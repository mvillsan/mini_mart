<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bg3"
    tools:context=".SearchSweetsID">

    <TextView
        android:id="@+id/rec_transparent"
        android:layout_width="190dp"
        android:layout_height="188dp"
        android:background="@drawable/rectangle_transparent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.927"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchProdBtnSS"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/copyrightTxt4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="52dp"
        android:fontFamily="@font/josefin_slab_light_italic"
        android:text="@string/title_credits"
        android:textColor="@android:color/darker_gray"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchProdQuantityTxt"
        app:layout_constraintVertical_bias="0.955" />

    <TextView
        android:id="@+id/searchTxt2"
        android:layout_width="223dp"
        android:layout_height="85dp"
        android:fontFamily="@font/bowlby_one"
        android:text="@string/title_searchProdIDbtn"
        android:textAlignment="textStart"
        android:textColor="@color/darkred"
        android:textSize="25sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.164"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView10"
        app:layout_constraintVertical_bias="0.022" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="151dp"
        android:layout_height="148dp"
        android:layout_marginBottom="356dp"
        android:contentDescription="@string/title_mmlogo"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.095"
        app:srcCompat="@drawable/logo" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="493dp"
        android:layout_height="60dp"
        android:contentDescription="@string/title_bar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.707"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/pastel_peach" />

    <TextView
        android:id="@+id/mmTxt4"
        android:layout_width="117dp"
        android:layout_height="33dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="228dp"
        android:fontFamily="@font/dalgona"
        android:shadowColor="@color/darkred"
        android:shadowDx="5"
        android:shadowDy="-5"
        android:shadowRadius="5"
        android:text="@string/title_minimart"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/homeBtn4"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@string/title_searchProd"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/imageView9"
        app:layout_constraintHorizontal_bias="0.155"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="0.028"
        app:srcCompat="@drawable/search_icon" />

    <EditText
        android:id="@+id/prodIDEditTxtSS"
        android:layout_width="179dp"
        android:layout_height="48dp"
        android:ems="10"
        android:fontFamily="@font/montserrat"
        android:hint="@string/title_enterProdID"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.129"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchTxt2"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/searchProdQuantityTxt"
        android:layout_width="172dp"
        android:layout_height="35dp"
        android:fontFamily="@font/raleway"
        android:text="@string/title_ProdQuantity"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.167"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchProdPriceTxt"
        app:layout_constraintVertical_bias="0.129" />

    <TextView
        android:id="@+id/searchProdNameTxt"
        android:layout_width="138dp"
        android:layout_height="32dp"
        android:fontFamily="@font/raleway"
        android:text="@string/title_ProdName"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchProdBtnSS"
        app:layout_constraintVertical_bias="0.131" />

    <TextView
        android:id="@+id/searchProdPriceTxt"
        android:layout_width="138dp"
        android:layout_height="32dp"
        android:fontFamily="@font/raleway"
        android:text="@string/title_ProdPrice"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/searchProdNameTxt"
        app:layout_constraintVertical_bias="0.103" />

    <TextView
        android:id="@+id/prodNameTxtSS"
        android:layout_width="138dp"
        android:layout_height="32dp"
        android:fontFamily="@font/raleway"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/rec_transparent"
        app:layout_constraintEnd_toEndOf="@+id/rec_transparent"
        app:layout_constraintHorizontal_bias="0.346"
        app:layout_constraintStart_toStartOf="@+id/rec_transparent"
        app:layout_constraintTop_toTopOf="@+id/rec_transparent"
        app:layout_constraintVertical_bias="0.134" />

    <TextView
        android:id="@+id/prodQuantTxtSS"
        android:layout_width="172dp"
        android:layout_height="35dp"
        android:fontFamily="@font/raleway"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/rec_transparent"
        app:layout_constraintEnd_toEndOf="@+id/rec_transparent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="@+id/rec_transparent"
        app:layout_constraintTop_toBottomOf="@+id/prodPriceTxtSS"
        app:layout_constraintVertical_bias="0.674" />

    <TextView
        android:id="@+id/prodPriceTxtSS"
        android:layout_width="138dp"
        android:layout_height="32dp"
        android:fontFamily="@font/raleway"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/rec_transparent"
        app:layout_constraintEnd_toEndOf="@+id/rec_transparent"
        app:layout_constraintHorizontal_bias="0.346"
        app:layout_constraintStart_toStartOf="@+id/rec_transparent"
        app:layout_constraintTop_toBottomOf="@+id/prodNameTxtSS"
        app:layout_constraintVertical_bias="0.277" />

    <ImageButton
        android:id="@+id/homeBtn4"
        android:layout_width="34dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:contentDescription="@string/title_homelogo"
        app:layout_constraintBottom_toBottomOf="@+id/imageView8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.042"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_icon" />

    <Button
        android:id="@+id/searchProdBtnSS"
        android:layout_width="181dp"
        android:layout_height="44dp"
        android:layout_marginBottom="100dp"
        android:background="@color/darkorange"
        android:drawableRight="@drawable/search"
        android:fontFamily="@font/lemon"
        android:text="@string/title_searchProd"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textColor="@color/lightgreen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.134"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/prodIDEditTxtSS"
        app:layout_constraintVertical_bias="0.05" />

</androidx.constraintlayout.widget.ConstraintLayout>